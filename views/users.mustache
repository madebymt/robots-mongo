<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <link rel="stylesheet" href="/css/user.css">
        <title></title>

    </head>
    <body>
        <h1>node hi</h1>
        <div class="users">
            {{#user}}
            <div class="show2">
            <img class="picture" src="{{avatar}}">

            <div class="info">
            <h2> {{name}}</h2>
             <p> {{address.city}}  |  {{address.country}} </p>
             <p> {{job}}   | {{company}}</p>
             <p> {{email}} | {{phone}}</p>
             </div>

             <div class="education">
             <h2 class="education"> Education : </h2> <h3>{{university}}</h3>
             {{^university}}{{.}}{{/university}}
             <h2 class="skill"> Skill : </h2> <h3>{{skills}}</h3>
             </div>
            {{/user}}

          </div>
        </div>
    </body>
</html>
